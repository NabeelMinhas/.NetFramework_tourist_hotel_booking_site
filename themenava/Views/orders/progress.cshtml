@model IEnumerable<themenava.Models.orders_inpregress>
@{
    if (Session["userID"] == null)
    {
        Response.Redirect("~/Home/login");
    }
    var a = Session["userID"];

}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>progress</title>
</head>
<body>
    <h1 style="text-align:center; color:red;padding-top:3rem;">Orders in Progress</h1>
    <p style="text-align:center;">
        @Html.ActionLink("Go Back", "Index", "dashboard")
    </p>
    <div class="container-fluid">
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.first_name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.last_name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.phone_no_1)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.phone_no_2)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.email_address)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.date)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.cnic)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.no_of_days)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.price)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.title)
                </th>
                <th>
                    Action
                </th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.first_name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.last_name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.phone_no_1)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.phone_no_2)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.email_address)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.date)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.cnic)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.no_of_days)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.price)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.title)
                    </td>
                    <td>
                        <a href="@Url.Action("del_and_add_for_complete","orders",new { item.id})">Complete</a>|
                        <a href="@Url.Action("delete_inprogress", "orders", new { item.id })">Delete</a>
                    </td>
                </tr>
            }

        </table>
    </div>
</body>
</html>
